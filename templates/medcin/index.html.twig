{% extends 'base.html.twig' %}

{% block title %}Medcin index{% endblock %}

{% block body %}
<h1><span class="fa fa-stethoscope" aria-hidden="true"></span>Medcin index</h1>


    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom_medcin</th>
                <th>Prenom_medcin</th>
                <th>Tel_medcin</th>
                <th>Email_medcin</th>
                <th>Cin_medcin</th>
                <th>Prix_visite</th>
                <th>Active</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for medcin in medcins %}
            <tr>
                <td>{{ medcin.id }}</td>
                <td>{{ medcin.nomMedcin }}</td>
                <td>{{ medcin.prenomMedcin }}</td>
                <td>{{ medcin.telMedcin }}</td>
                <td>{{ medcin.emailMedcin }}</td>
                <td>{{ medcin.cinMedcin }}</td>
                <td>{{ medcin.prixVisite }}</td>
                <td>{{ medcin.active }}</td>
                <td><div class="login-icon ml-2">
                    <a class="" href="{{ path('medcin_show', {'id': medcin.id}) }}"> show </a>
                    <a class="" href="{{ path('medcin_edit', {'id': medcin.id}) }}"> edit </a>
                    </div>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
<div class="login-icon ml-2">
<a class="" href="{{ path('medcin_new') }}"> Create new </a>
</div>

<div class="login-icon ml-2">
<a class="" href="{{ path('admin') }}"> Page admin </a>
</div>
{% endblock %}
