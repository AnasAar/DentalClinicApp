{% extends 'base.html.twig' %}

{% block title %}DossierMedical index{% endblock %}

{% block body %}
<h1><span class="fa fa-stethoscope" aria-hidden="true"></span>Dossier Medical index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Cnss</th>
                <th>Cnops</th>
                <th>Obsservation</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for dossier_medical in dossier_medicals %}
            <tr>
                <td>{{ dossier_medical.id }}</td>
                <td>{{ dossier_medical.cnss }}</td>
                <td>{{ dossier_medical.cnops }}</td>
                <td>{{ dossier_medical.obsservation }}</td>
                <td><div class="login-icon ml-2">
                    <a class="" href="{{ path('dossier_medical_show', {'id': dossier_medical.id}) }}"> show </a>
                    <a class="" href="{{ path('dossier_medical_edit', {'id': dossier_medical.id}) }}"> edit </a>
                    </div>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
<div class="login-icon ml-2">
<a class="" href="{{ path('dossier_medical_new') }}"> Create new </a>
</div>
<div class="login-icon ml-2">
<a class="" href="{{ path('admin') }}"> Page admin </a>
</div>
{% endblock %}
