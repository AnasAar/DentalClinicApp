{% extends 'base.html.twig' %}

{% block title %}Ligne traitement index{% endblock %}

{% block body %}
<h1><span class="fa fa-stethoscope" aria-hidden="true"></span>Ligne traitement index</h1>


    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom_dent</th>
                <th>Date_debut</th>
                <th>Date_fin</th>
                <th>Type_traitement</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for lignetraitement in lignetraitements %}
            <tr>
                <td>{{ lignetraitement.id }}</td>
                <td>{{ lignetraitement.nomDent }}</td>
                <td>{{ lignetraitement.dateDebut ? lignetraitement.dateDebut|date('Y-m-d') : '' }}</td>
                <td>{{ lignetraitement.dateFin ? lignetraitement.dateFin|date('Y-m-d') : '' }}</td>
                <td>{{ lignetraitement.typeTraitement }}</td>
                <td><div class="login-icon ml-2">
                    <a class="" href="{{ path('lignetraitement_show', {'id': lignetraitement.id}) }}"> show </a>
                    <a class="" href="{{ path('lignetraitement_edit', {'id': lignetraitement.id}) }}"> edit </a>
                    </div>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
<div class="login-icon ml-2">
<a class="" href="{{ path('lignetraitement_new') }}"> Create new </a>
</div>

<div class="login-icon ml-2">
<a class="" href="{{ path('admin') }}"> Page admin </a>
</div>
{% endblock %}
